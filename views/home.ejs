<section id="feed-posts">
<%if (locals.user &&  locals.user.type=="recruiter"){%>

<h2 class="bg-primary mt-3 text-center">Since you are a recruiter you can only post jobs but not apply </h2>

<h2 class="ml-5"> Recruitung Details...</h2>
	<form class="ml-5 " action="/posts/create "  id="new-post-form" method="post">
  <div class="form-group bg-dark text-white ">
    <label for="exampleFormControlSelect1">Employment Type</label>
    <select name="employmentType"class="form-control bg-dark text-white" id="exampleFormControlSelect1">
      <option>Daily Wages</option>
      <option>Internships</option>
      <option>Full time role</option>
    </select>
  </div>
  <div class="form-group bg-dark text-white">
    <label for="exampleFormControlInput1">Profile/Work</label>
    <input name="profile"type="text" class="form-control bg-dark text-white" id="exampleFormControlInput1" placeholder="Web Developer">
  </div>
  <div class="form-group bg-dark text-white">
    <label for="exampleFormControlInput1">Salary</label>
    <input name="salary"type="number" class="form-control bg-dark text-white" id="exampleFormControlInput1" >
  </div>

  <div class="form-group bg-dark text-white">
    <label for="exampleFormControlSelect1">Per</label>
    <select name="per"class="form-control bg-dark text-white" id="exampleFormControlSelect1">
      <option>daily</option>
      <option>month</option>
      <option>annualy</option>
      
    </select>
  </div>
  
  <div class="form-group bg-dark text-white">
    <label for="exampleFormControlTextarea1">Requirements</label>
    <textarea name="requirements" class="form-control bg-dark text-white" id="exampleFormControlTextarea1" rows="3"></textarea>
  </div>
  <input type="submit" name="Submit">
</form>

<%}%>

<div id="posts-container-list " class="container-fluid pt-2  ">

	<div class="section_header text-center text-light h1 bg1 btn-outline-dark ">
       <a class="  text-light btn-outline-dark" href="/jobs/all">New Jobs..</a>
    </div>
<div class="container bg-light " >
	<div class="row bg3 "  style="flex-wrap: nowrap;overflow-x: scroll">
		<% for (let i=posts.length-1;i>=0;i--) {%>

		<div class="col-lg-4 col-md-4 col-6 mt-3 border border-light mr-4 text-light bg2">

           <p class="pt-2">
             Company:<%=posts[i].user.name %>
             <br>
             Profile:<%=posts[i].profile%>
             <br>
             <span style="overflow:wrap">Essentials:
             <%=posts[i].requirements%></span>
             <br>
             Pay:<%=posts[i].salary%> per <%=posts[i].per%>
             <br>
             
             
            <%if(locals.user && locals.user.type=="jobseeker"){%>
          
              <form action="/posts/<%=posts[i]._id%>/<%=user._id%>" method="get">
             	<button>Apply</button>
             </form>

             <%} else if(!locals.user) {%>
             <form action="/users/jobseekerSignin" method="get">
             	<button>Apply</button>
             </form>
             <%}%>
             
           </p>


<%if(locals.user && locals.user.id==posts[i].user.id){%>

<a class="delete-post-button" href="/posts/destroy/<%=posts[i].id%>">X </a>


<%}%>
</div>
		 <%}%>
	</div>
</div>

  
</div>



